<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:background="@android:color/white"
    android:layout_height="wrap_content">

    <View
        android:id="@+id/viewSquare11"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:background="@android:color/white"
        app:layout_constraintHorizontal_weight="0.3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view11"
        android:layout_width="0dp"
        android:layout_height="14dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:background="@android:color/white"
        app:layout_constraintHorizontal_weight="0.5"
        app:layout_constraintLeft_toRightOf="@id/viewSquare11"
        app:layout_constraintRight_toLeftOf="@id/space11"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/space11"
        android:layout_width="0dp"
        android:layout_height="14dp"
        app:layout_constraintBottom_toBottomOf="@id/view11"
        app:layout_constraintHorizontal_weight="0.5"
        app:layout_constraintLeft_toRightOf="@id/view11"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/view11" />

    <View
        android:id="@+id/view12"
        android:layout_width="0dp"
        android:layout_height="7dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:background="@android:color/white"
        app:layout_constraintHorizontal_weight="0.2"
        app:layout_constraintLeft_toRightOf="@id/viewSquare11"
        app:layout_constraintRight_toLeftOf="@id/space12"
        app:layout_constraintTop_toBottomOf="@id/view11" />

    <View
        android:id="@+id/space12"
        android:layout_width="0dp"
        android:layout_height="7dp"
        app:layout_constraintBottom_toBottomOf="@id/view12"
        app:layout_constraintHorizontal_weight="0.5"
        app:layout_constraintLeft_toRightOf="@id/view12"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/view12" />

    <View
        android:id="@+id/view13"
        android:layout_width="0dp"
        android:layout_height="14dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:background="@android:color/white"
        app:layout_constraintHorizontal_weight="0.7"
        app:layout_constraintLeft_toRightOf="@id/viewSquare11"
        app:layout_constraintRight_toLeftOf="@id/space13"
        app:layout_constraintTop_toBottomOf="@id/view12" />

    <View
        android:id="@+id/space13"
        android:layout_width="0dp"
        android:layout_height="14dp"
        app:layout_constraintBottom_toBottomOf="@id/view13"
        app:layout_constraintHorizontal_weight="0.3"
        app:layout_constraintLeft_toRightOf="@id/view13"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/view13" />

    <View
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginTop="20dp"
        android:layout_marginHorizontal="20dp"
        android:background="@android:color/white"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/viewSquare11" />

</androidx.constraintlayout.widget.ConstraintLayout>

